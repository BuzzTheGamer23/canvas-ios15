{
  "name": "Teacher",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start-iphone": "react-native run-ios --simulator='iPhone 8' --project-path ../../ --scheme Teacher",
    "start-iphone-x": "react-native run-ios --simulator='iPhone X' --project-path ../../ --scheme Teacher",
    "start-ipad": "react-native run-ios --simulator='iPad Pro (10.5-inch)' --project-path ../../ --scheme Teacher",
    "build": "yarn && pod install",
    "build-update": "yarn && pod install --repo-update",
    "clean": "rm -rf \"$HOME/Library/Developer/Xcode/DerivedData/\" && rm -rf './node_modules' && rm -rf '../../build' && pod cache clean --all",
    "test": "jest",
    "lint": "eslint *.js __tests__/*.js src test flow",
    "lint:fix": "eslint --fix *.js __tests__/*.js src test",
    "lint:watch": "esw -w",
    "flow": "flow check --show-all-errors | flow-result-checker",
    "extract-strings": "format-message extract \"src/**/*.js\" index.ios.js -o i18n/locales/en.json",
    "danger": "danger",
    "packager:kill": "react-native-kill-packager",
    "postinstall": "./fix-rn-crap.sh",
    "prepush": "yarn test --coverage && yarn lint:fix && yarn flow"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "bytes": "https://github.com/n8armstrong/bytes.js",
    "create-react-class": "15.6.2",
    "filesize": "^3.5.10",
    "flux-standard-action": "^1.2.0",
    "format-message": "^5.1.1",
    "global": "^4.3.2",
    "hoist-non-react-statics": "^1.2.0",
    "knuth-shuffle-seeded": "^1.0.6",
    "lodash": "^4.17.4",
    "md5": "^2.2.1",
    "prop-types": "^15.6.0",
    "querystring": "^0.2.0",
    "react": "16.0.0-beta.5",
    "react-native": "0.49.3",
    "react-native-audio": "https://github.com/derrh/react-native-audio.git",
    "react-native-blur": "^3.1.2",
    "react-native-button": "^2.1.0",
    "react-native-camera": "^0.10.0",
    "react-native-counter": "https://github.com/instructure/react-native-counter",
    "react-native-device-info": "^0.10.1",
    "react-native-document-picker": "https://github.com/Elyx0/react-native-document-picker.git",
    "react-native-fs": "^2.3.2",
    "react-native-image-picker": "^0.26.3",
    "react-native-interactable": "https://github.com/derrh/react-native-interactable.git",
    "react-native-keyboard-aware-scroll-view": "^0.3.0",
    "react-native-keyboard-spacer": "https://github.com/n8armstrong/react-native-keyboard-spacer.git",
    "react-native-linear-gradient": "^2.2.0",
    "react-native-mail": "https://github.com/instructure/react-native-mail",
    "react-native-on-layout": "^2.0.2",
    "react-native-progress": "^3.3.0",
    "react-native-search-bar": "https://github.com/instructure/react-native-search-bar",
    "react-native-sfsafariviewcontroller": "https://github.com/derrh/react-native-sfsafariviewcontroller.git",
    "react-native-sound": "^0.10.4",
    "react-native-test-utils": "^1.0.6",
    "react-native-wkwebview-reborn": "https://github.com/instructure/react-native-wkwebview",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.2",
    "route-parser": "^0.0.5",
    "striptags": "^3.0.1",
    "url-parse": "^1.1.9",
    "uuid": "^3.1.0",
    "valid-url": "1.0.9",
    "xmldom": "^0.1.27"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-preset-react-native": "2.1.0",
    "danger": "https://github.com/benkraus/danger-js/tarball/with-distribution",
    "eslint": "^3.16.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.0.1",
    "eslint-watch": "^3.0.0",
    "flow-bin": "^0.59.0",
    "flow-result-checker": "0.3.0",
    "format-message-cli": "^5.1.1",
    "format-message-generate-id": "^5.1.0",
    "jest": "^20.0.4",
    "react-native-kill-packager": "^1.0.0",
    "react-test-renderer": "16.0.0-alpha.12",
    "redux-freeze": "^0.1.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.2.3",
    "timezone-mock": "^0.0.4",
    "xhr2": "^0.1.4"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|my-project|react-native-button)"
    ],
    "setupFiles": [
      "<rootDir>/test/scripts/jestSetup.js"
    ],
    "moduleNameMapper": {
      "^.+\\.html$": "<rootDir>/test/scripts/html-mock"
    },
    "verbose": true,
    "collectCoverage": false,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      ".android.js",
      "flow/*.js",
      "/src/modules/staging",
      "dangerfile.js",
      "/src/canvas-api"
    ],
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "json",
      "json-summary",
      "lcov",
      "text",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 80,
        "lines": 90,
        "statements": 90
      }
    }
  }
}
